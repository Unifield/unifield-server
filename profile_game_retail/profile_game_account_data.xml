<?xml version="1.0" ?>
<openerp>
	<data>
		<record id="account_type_asset0" model="account.account.type">
            <field name="name">Asset</field>
            <field name="code">asset</field>
            <field name="close_method">balance</field>
        </record>
        <record id="account_type_liability0" model="account.account.type">
            <field name="name">Liability</field>
            <field name="code">liability</field>
            <field name="close_method">balance</field>
        </record>

         <record id="account_type_income0" model="account.account.type">
            <field name="name">Income</field>
            <field name="code">income</field>
            <field name="close_method">unreconciled</field>
        </record>
         <record id="account_type_expense0" model="account.account.type">
            <field name="name">Expense</field>
            <field name="code">expense</field>
            <field name="close_method">unreconciled</field>
        </record>
        <record id="account_type_cash_moves0" model="account.account.type">
            <field name="name">Cash</field>
            <field name="code">cash</field>
            <field name="close_method">balance</field>
        </record>
	 <!--
        Journal Sales and purchase
        -->
		<record id="account_journal_income" model="account.journal">
            <field name="name">x Sales Journal</field>
            <field name="code">SAJ</field>
            <field name="type">sale</field>
            <field name="view_id" ref="account.account_journal_view"/>
            <field name="sequence_id" ref="account.sequence_sale_journal"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
 		<record id="account_expenses_journal" model="account.journal">
            <field name="name">x Expenses Journal</field>
            <field name="code">EXJ</field>
            <field name="type">purchase</field>
            <field name="view_id" ref="account.account_journal_view"/>
            <field name="sequence_id" ref="account.sequence_purchase_journal"/>
            <field name="user_id" ref="base.user_root"/>
        </record>

 	<!--
        Account Receivable and payable
        -->
		<record id="account_cash" model="account.account">
            <field name="name">Petty Cash</field>
            <field name="code">x 570000</field>
            <field name="type">other</field>
            <field name="user_type" ref="account_type_cash_moves0"/>
            <field name="company_id" ref="base.main_company"/>
            <field eval="True" name="reconcile"/>
            <field name="user_type" ref="account_type_cash_moves0"/>
		</record>

		<record id="account_recv" model="account.account">
            <field name="name">Main Receivable</field>
            <field name="code">x 40000</field>
            <field name="type">receivable</field>
            <field name="user_type" ref="account_type_asset0"/>
            <field name="company_id" ref="base.main_company"/>
            <field eval="True" name="reconcile"/>
            <field name="user_type" ref="account_type_income0"/>
        </record>

        <record id="account_pay" model="account.account">
            <field name="name">Main Payable</field>
            <field name="code">x 440000</field>
            <field name="type">payable</field>
            <field name="user_type" ref="account_type_liability0"/>
            <field name="company_id" ref="base.main_company"/>
            <field eval="True" name="reconcile"/>
            <field name="user_type" ref="account_type_expense0"/>
        </record>

         <record id="account_bank_journal" model="account.journal">
            <field name="name">x Bank Journal</field>
            <field name="code">BNK</field>
            <field name="type">cash</field>
            <field name="view_id" ref="account.account_journal_bank_view"/>
            <field name="sequence_id" ref="account.sequence_journal"/>
            <field name="default_debit_account_id" ref="account_cash"/>
            <field name="default_credit_account_id" ref="account_cash"/>
            <field name="user_id" ref="base.user_root"/>
        </record>
     <!--
	Fiscal year
	-->

        <record id="data_fiscalyear_game" model="account.fiscalyear">
            <field eval="'Fiscal Year '+time.strftime('%Y')" name="name"/>
            <field eval="'FY'+time.strftime('%Y')" name="code"/>
            <field eval="time.strftime('%Y')+'-01-01'" name="date_start"/>
            <field eval="time.strftime('%Y')+'-12-31'" name="date_stop"/>
        </record>

        <!--
	Fiscal Periods
	-->

        <record id="game_period_1" model="account.period">
            <field eval="'Jan.'+time.strftime('%Y')" name="name"/>
            <field name="fiscalyear_id" ref="data_fiscalyear_game"/>
            <field eval="time.strftime('%Y')+'-01-01'" name="date_start"/>
