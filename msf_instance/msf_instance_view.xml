<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        
        <record id="view_msf_instance_form" model="ir.ui.view">
            <field name="name">msf.instance.form</field>
            <field name="model">msf.instance</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Proprietary Instance" hide_duplicate_button="1" hide_delete_button="1">
                    <field name="code"/>
                    <field name="name"/>
                       <newline/>
                    <field name="level"/>
                    <newline />
                    <field name="mission" attrs="{'required': [('level', '!=', 'section')], 'readonly': [('level', '=', 'section')]}"/>
                    <field name="instance" readonly="1"/>
                    <newline/>
                    <field name="parent_id" attrs="{'required': [('level', '!=', 'section')], 'readonly': [('level', '=', 'section')]}"/>
                    <field name="note"/>
                    <newline/>
                    <field name="cost_center_id"/>
                    <field name="state" readonly="1"/>
                    <button name="button_deactivate" string="Deactivate" type="object"
                            attrs="{'invisible': [('state', '!=', 'active')]}" icon="terp-check"/>
                </form>
            </field>
        </record>
        
        <record id="view_msf_instance_tree" model="ir.ui.view">
            <field name="name">msf.instance.tree</field>
            <field name="model">msf.instance</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Proprietary Instances" hide_delete_button="1">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="level"/>
                    <field name="mission"/>
                    <field name="instance"/>
                    <field name="parent_id"/>
                    <field name="cost_center_id"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        
        <record id="action_msf_instance_tree" model="ir.actions.act_window">
            <field name="name">Proprietary Instances</field>
            <field name="res_model">msf.instance</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        
        <menuitem action="action_msf_instance_tree" id="menu_action_msf_instance_tree" parent="base.menu_config"/>
        

        <record model="ir.ui.view" id="view_company_inherit_4_form">
            <field name="name">res.company.form.instance.inherit</field>
            <field name="inherit_id" ref="account_msf.view_company_inherit_3_form"/>
            <field name="model">res.company</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <page string="Configuration" position="inside">
                  <group col="2" colspan="2">
                      <separator string="Proprietary Instance" colspan="2"/>
                      <field name="instance_id" required="1" colspan="2" domain="[('state', '=', 'draft')]"/>
                  </group>
                </page>
            </field>
        </record>

    <!-- Redefine analytic line tree -->
        <record id="view_account_analytic_line_instance_tree" model="ir.ui.view">
            <field name="name">account.analytic.line.instance.tree</field>
            <field name="model">account.analytic.line</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="account.view_account_analytic_line_tree"/>
            <field name="arch" type="xml">
                <field name="journal_id" position="before">
                    <field name="instance_id" readonly="1"/>
                </field>
            </field>
        </record>

    <!-- Redefine analytic journal -->
        <record id="view_account_analytic_journal_instance_tree" model="ir.ui.view">
            <field name="name">account.analytic.journal.instance.tree</field>
            <field name="model">account.analytic.journal</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="account.view_account_analytic_journal_tree"/>
            <field name="arch" type="xml">
                <field name="code" position="before">
                    <field name="instance_id" readonly="1"/>
                </field>
            </field>
        </record>
        
        <record id="view_account_analytic_journal_instance_form" model="ir.ui.view">
            <field name="name">account.analytic.journal.instance.form</field>
            <field name="model">account.analytic.journal</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="account.view_account_analytic_journal_form"/>
            <field name="arch" type="xml">
                <field name="code" position="after">
                    <field name="instance_id" readonly="1"/>
                </field>
            </field>
        </record>
        
    <!-- Redefine G/L journal -->
        <record id="view_account_journal_form_instance_inherit" model="ir.ui.view">
            <field name="name">account.journal.form.instance.inherit</field>
            <field name="model">account.journal</field>
            <field name="type">form</field>
            <field name="priority" eval="250" />
            <field name="inherit_id" ref="account_journal.view_account_journal_form_inherit"/>
            <field name="arch" type="xml">
                <separator string="Company" position="after">
                    <field name="instance_id" readonly="1"/>
                </separator>
            </field>
        </record>
        
        <record id="view_account_journal_tree_instance_inherit" model="ir.ui.view">
            <field name="name">account.journal.tree.instance.inherit</field>
            <field name="model">account.journal</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="account_journal.view_account_journal_tree_inherit"/>
            <field name="arch" type="xml">
                <field name="code" position="before">
                    <field name="instance_id" readonly="1"/>
                </field>
            </field>
        </record>
        
    <!-- Redefine G/L account move -->
        <record id="view_move_form_instance_inherit" model="ir.ui.view">
            <field name="name">account.move.form.instance.inherit</field>
            <field name="model">account.move</field>
            <field name="type">form</field>
            <field name="priority" eval="250" />
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <form string="Journal Entries" position="replace">
	               <form string="Journal Entries" hide_new_button="1" hide_duplicate_button="1" hide_delete_button="1">
	                    <group colspan="4" col="6">
                            <field name="instance_id" readonly="1"/>
                            <field name="journal_id" readonly="1"/>
                            <field name="name" readonly="1"/>
                            <field name="period_id" readonly="1"/>
	                        <field name="ref" readonly="1"/>
	                        <field name="partner_id" invisible="1"/>
	                        <field name="amount" invisible="1"/>
	                    </group>
	                    <notebook colspan="4">
	                        <page string="Journal Items">
	                            <field colspan="4" name="line_id" nolabel="1" height="250" widget="one2many_list" default_get="{'lines':line_id , 'journal':journal_id, 'period_id':period_id}">
	                                <tree colors="blue:state in ('draft');black:state in ('posted')" editable="top" hide_new_button="1" hide_delete_button="1" noteditable="1" string="Journal Items">
                                        <field name="name"/>
	                                    <field name="ref"/>
                                        <field name="document_date"/>
                                        <field name="date"/>
	                                    <field name="account_id"/>
				                        <field name="partner_id" invisible="1"/>
				                        <field name="partner_type"/>
	                                    <field name="debit_currency" sum="Total Booking Debit" string="Book. Debit"/>
				                        <field name="credit_currency" sum="Total Booking Credit" string="Book. Credit"/>
				                        <field name="currency_id" string="Book. Currency"/>
				                        <field name="debit" sum="Total Functional Debit" string="Func. Debit" readonly="1"/>
				                        <field name="credit" sum="Total Functional Credit" string="Func. Credit" readonly="1"/>
				                        <field name="functional_currency_id" readonly="1"/>
                                        <field name="reconcile_partial_id" string="Reconcile"/>
	                                    <field name="state"/>
	                                </tree>
	                            </field>
	                        </page>
	                    </notebook>
	                </form>
                </form>
            </field>
        </record>
        
        <record id="view_move_tree_instance_inherit" model="ir.ui.view">
            <field name="name">account.move.tree.instance.inherit</field>
            <field name="model">account.move</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="account.view_move_tree"/>
            <field name="arch" type="xml">
                <tree string="Journal Entries" position="replace">
                    <tree colors="blue:state in ('draft');black:state in ('posted')" string="Journal Entries" noteditable="1" hide_new_button="1" hide_delete_button="1">
                        <field name="instance_id" readonly="1"/>
                        <field name="name"/>
                        <field name="ref"/>
                        <field name="date"/>
                        <field name="period_id"/>
                        <field name="journal_id"/>
                        <field name="partner_id" invisible="1"/>
                        <field name="partner_type"/>
                        <field name="amount" sum="Total Amount"/>
                        <field name="state"/>
                    </tree>
                </tree>
            </field>
        </record>
        
    <!-- Journal Entries Search (filter) -->
        <record model="ir.ui.view" id="inherit_view_account_move_filter">
            <field name="name">inherit.view.account.move.filter</field>
            <field name="model">account.move</field>
            <field name="type">search</field>
            <field name="inherit_id" ref="account.view_account_move_filter" />
            <field name="arch" type="xml">
                <xpath expr="//filter[@string='To Review']" position="replace"/>
                <xpath expr="//field[@name='partner_id']" position="replace">
                    <field name="partner_id" invisible="1"/>
                    <field name="partner_type"/>
                </xpath>
            </field>
        </record>
        
    <!-- Redefine G/L account move line -->
        <record id="view_move_line_tree_instance_inherit" model="ir.ui.view">
            <field name="name">account.move.line.tree.instance.inherit</field>
            <field name="model">account.move.line</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="res_currency_functional.view_move_line_tree"/>
            <field name="arch" type="xml">
                <field name="journal_id" position="before">
                    <field name="instance_id" readonly="1"/>
                </field>
            </field>
        </record>
        
        <record id="view_move_line_tree_editable_instance_inherit" model="ir.ui.view">
            <field name="name">account.move.line.tree.editable.instance.inherit</field>
            <field name="model">account.move.line</field>
            <field name="type">tree</field>
            <field name="inherit_id" ref="res_currency_functional.view_move_line_tree_editable"/>
            <field name="arch" type="xml">
                <field name="journal_id" position="before">
                    <field name="instance_id" readonly="1"/>
                </field>
            </field>
        </record>
        
    <!-- Redefine register -->
        <record id="inherit_view_bank_statement_instance_form" model="ir.ui.view">
            <field name='name'>inherit.view.bank.statement.instance.form</field>
            <field name='model'>account.bank.statement</field>
            <field name="type">form</field>
            <field name='inherit_id' ref='register_accounting.inherit_view_bank_statement_form' />
            <field name="priority" eval="20"/>
            <field name="arch" type="xml">
                <data>
	                <xpath expr="/form//field[@name='journal_id']" position="before">
	                    <field name="instance_id" readonly="1"/>
	                </xpath>
                </data>
            </field>
        </record>
        
        <record id="inherit_view_bank_statement_instance_form2" model="ir.ui.view">
            <field name='name'>inherit.view.bank.statement.instance.form2</field>
            <field name='model'>account.bank.statement</field>
            <field name="type">form</field>
            <field name='inherit_id' ref='register_accounting.inherit_view_bank_statement_form2' />
            <field name="priority" eval="20"/>
            <field name="arch" type="xml">
                <data>
	                <xpath expr="/form//field[@name='journal_id']" position="before">
	                    <field name="instance_id" readonly="1"/>
	                </xpath>
                </data>
            </field>
        </record>
        
        <record id="view_cheque_register_form" model="ir.ui.view">
            <field name='name'>view.cheque.register.instance.form</field>
            <field name='model'>account.bank.statement</field>
            <field name="type">form</field>
            <field name='inherit_id' ref='register_accounting.view_cheque_register_form'/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="/form//field[@name='journal_id']" position="before">
                        <field name="instance_id" readonly="1"/>
                    </xpath>
                </data>
            </field>
        </record>
        
        <record id="view_bank_statement_instance_tree" model="ir.ui.view">
            <field name="name">account.bank.statement.instance.tree</field>
            <field name="model">account.bank.statement</field>
            <field name="type">tree</field>
            <field name="priority" eval="16"/>
            <field name='inherit_id' ref='account.view_bank_statement_tree' />
            <field name="arch" type="xml">
                <xpath expr="/tree/field[@name='name']" position="before">
