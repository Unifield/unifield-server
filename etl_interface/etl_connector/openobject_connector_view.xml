<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1">
		<record id="conn_openobject_connector"
			model="etl.connector.type">
			<field name="name">Openobject</field>
			<field name="code">openobject_connector</field>
		</record>
	</data>
	<data>

		<record id="etl_connector_openobject_form" model="ir.ui.view">
			<field name="name">etl.connector.openobject.form</field>
			<field name="model">etl.connector</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="OpenObject Connector">
					<group colspan="4">
						<field name="name" select="1" />
						<field name="type" invisible="1"
							domain="[('type','=','openobject_connector')]"
							on_change="onchange_type(type)" />
					</group>
					<notebook colspan="4">
						<page string="Property">
							<field name="uri" select="1" required="1" />
							<field name="db" required="1" />
							<field name="login" required="1" />
							<field name="passwd" password="True"
								required="1" />
							<field name="obj" required="1" />
							<field name="con_type" required="1" />
						</page>
					</notebook>
				</form>
			</field>
		</record>



		<record id="action_etl_connector_openobject_tree"
			model="ir.actions.act_window">
			<field name="name">Openobject Connectors</field>
			<field name="res_model">etl.connector</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('type','=','openobject_connector')]</field>
			<field name="view_id"
				ref="etl_interface.etl_connector_basic_tree" />
		</record>

		<record id="action_etl_connector_openobject_view1"
			model="ir.actions.act_window.view">
			<field eval="1" name="sequence" />
			<field name="view_mode">tree</field>
			<field name="view_id"
				ref="etl_interface.etl_connector_basic_tree" />
			<field name="act_window_id"
				ref="action_etl_connector_openobject_tree" />
		</record>

		<record id="action_etl_connector_openobject_view2"
			model="ir.actions.act_window.view">
			<field eval="2" name="sequence" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="etl_connector_openobject_form" />
			<field name="act_window_id"
				ref="action_etl_connector_openobject_tree" />
		</record>

		<menuitem name="Open Object Connector"
			parent="etl_interface.etl_connector_all_tree"
			action="action_etl_connector_openobject_tree"
			id="connector_openobject_control" />

	</data>
</openerp>

