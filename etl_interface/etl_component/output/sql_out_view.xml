<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <record id="comp_output_sql_out" model="etl.component.type">
			<field name="name">output.sql_out</field>
			<field name="code">output.sql_out</field>
		</record>
    </data>
	<data>

		<record id="etl_component_sql_out_form" model="ir.ui.view">
			<field name="name">etl.component.sql.out.form</field>
			<field name="model">etl.component</field>
			<field name="type">form</field>           
			<field name="arch" type="xml">
                <form string="Sql Out Component">
                    <group colspan="4">
					    <field name="name" select="1" />
					    <field name="type_id" invisible="1" domain="[('type_id.name','=','output.sql_out')]"/>
                        <field name="transformer_id" invisible="1" colspan="4"/>
                    </group>                    
                    <notebook colspan="4">                        
                        <page string="Property">
                            <field name="connector_id"/>					
					        <field name="row_limit" />					         					   
                        </page>
                        <page string="Transitions Detail">
					        <separator string="Incomming Transitions" colspan="4" />
					        <field name="trans_in_ids" nolabel="1" colspan="4" readonly="1" />
					        <separator string="Outgoing Transitions" colspan="4" />
					        <field name="trans_out_ids" nolabel="1" colspan="4" readonly="1" />
                        </page>
                    </notebook>
				</form>									
			</field>
		</record>

		<record id="action_etl_component_sql_out_tree"
			model="ir.actions.act_window">
			<field name="name">SQL Out component</field>
			<field name="res_model">etl.component</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('type_id.name','=','output.sql_out')]</field>
		</record>

		<record id="action_etl_component_sql_out_from"
			model="ir.actions.act_window">
			<field name="res_model">etl.component</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
			<field name="domain">[('type_id.name','=','output.sql_out')]</field>
			<field name="view_id" ref="etl_component_sql_out_form" />
		</record>

		<record id="action_etl_component_sql_out_view1"
			model="ir.actions.act_window.view">
			<field eval="1" name="sequence" />
			<field name="view_mode">tree</field>
			<field name="view_id" ref="etl_interface.etl_component_tree" />
			<field name="act_window_id" 	ref="action_etl_component_sql_out_tree" />
		</record>
		<record id="action_etl_component_sql_out_view2" model="ir.actions.act_window.view">
			<field eval="2" name="sequence" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="etl_component_sql_out_form" />
			<field name="act_window_id" ref="action_etl_component_sql_out_tree" />
		</record>


		<menuitem name="SQL Output Data " parent="etl_interface.menu_etl_jd_c_oc_form"
			action="action_etl_component_sql_out_tree" 	id="menu_etl_jd_c_oc_sod_form" />		

	</data>
</openerp>

