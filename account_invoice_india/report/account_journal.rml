  <story>
    <para style="P10">[[ repeatIn(objects, 'o') ]]</para>
    <blockTable colWidths="482.0" repeatRows="1" style="Table5">
      <tr>
        <td>
          <para style="P7">[[ company.name ]]</para>
          <para style="P11">[[ company.partner_id.address and company.partner_id.address[0].street ]]</para>
          <para style="P11">[[ company.partner_id.address and company.partner_id.address[0].city ]] - [[ company.partner_id.address and company.partner_id.address[0].zip ]] , [[ company.partner_id.address and company.partner_id.address[0].country_id and company.partner_id.address[0].country_id.name ]]</para>
          <para style="P13">Phone : [[ company.partner_id.address and company.partner_id.address[0].phone ]] Email :[[ company.partner_id.address and company.partner_id.address[0].email ]]</para>
        </td>
      </tr>
    </blockTable>
    <para style="Standard">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="128.0,222.0,131.0" repeatRows="1" style="Table1">
      <tr>
        <td>
          <para style="Table Contents">[[o.period_id.name ]]</para>
        </td>
        <td>
          <para style="P15">[[ o.journal_id.name ]]</para>
        </td>
        <td>
          <para style="P1">Currency: [[ company.currency_id.name ]]</para>
        </td>
      </tr>
    </blockTable>
    <para style="P9">
      <font color="white"> </font>
    </para>
    <blockTable colWidths="49.0,48.0,77.0,82.0,108.0,58.0,60.0" repeatRows="1" style="Table2">
      <tr>
        <td>
          <para style="P8">Date</para>
        </td>
        <td>
          <para style="P8">Voucher Nb</para>
        </td>
        <td>
          <para style="P8">Third Party</para>
        </td>
        <td>
          <para style="P8">Account</para>
        </td>
        <td>
          <para style="P8">Narration</para>
        </td>
        <td>
          <para style="P8">Debit</para>
        </td>
        <td>
          <para style="P8">Credit</para>
        </td>
      </tr>
      <tr>
        <td>
          <para style="P3">
            <font face="Times-Roman" size="8.0">[[ repeatIn(lines(o.period_id.id, o.journal_id.id), 'line') ]]</font>[[ line.date ]]
          </para>
        </td>
        <td>
          <para style="P3">[[ line.ref ]]</para>
        </td>
        <td>
          <para style="P3">[[ line.partner_id and line.partner_id.name ]]</para>
        </td>
        <td>
