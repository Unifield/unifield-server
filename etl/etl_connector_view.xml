<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<!--	#################   Connector Types 	###################-->

		<record id="etl_connector_type_form" model="ir.ui.view">
			<field name="name">view.etl.connector.type.form</field>
			<field name="model">etl.connector.type</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="etl.connector.type">
					<field name="name" />
					<field name="code" />
					<separator string="Arguments" colspan="4" />
					<field name="args" nolabel="1" colspan="4" />
				</form>
			</field>
		</record>


		<record id="etl_connector_type_tree" model="ir.ui.view">
			<field name="name">view.etl.connector.type.tree</field>
			<field name="model">etl.connector.type</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="etl.connector.type">
					<field name="name" />
					<field name="code" />
				</tree>
			</field>
		</record>

		<record id="action_etl_connector_type_tree"
			model="ir.actions.act_window">
			<field name="name">Connector Types</field>
			<field name="res_model">etl.connector.type</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="etl_connector_type_tree" />
		</record>


		<menuitem name="Connector Types" parent="menu_etl_configure"
			sequence="8" action="action_etl_connector_type_tree"
			id="menu_etl_connector_type" />

		<!--	#################   Connectors 	###################-->

		<record id="etl_connector_basic_form" model="ir.ui.view">
			<field name="name">etl.connector.basic.form</field>
			<field name="model">etl.connector</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="etl.connector">
					<field name="name" select="1" />
					<field name="type" select="1" on_change="onchange_type(type)" />
				</form>
			</field>
		</record>


		<record id="etl_connector_basic_tree" model="ir.ui.view">
			<field name="name">etl.connector.basic.tree</field>
			<field name="model">etl.connector</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Connector">
					<field name="name" select="1" />
					<field name="type" select="1" />
				</tree>
			</field>
		</record>

		<record id="action_etl_connector_basic_tree"
			model="ir.actions.act_window">
			<field name="name">All Connectors</field>
			<field name="res_model">etl.connector</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="etl_connector_basic_tree" />
		</record>
		
			<record id="action_etl_connector_basic_view1"
			model="ir.actions.act_window.view">
			<field eval="1" name="sequence" />
			<field name="view_mode">tree</field>
			<field name="view_id" ref="etl_connector_basic_tree" />
			<field name="act_window_id"
				ref="action_etl_connector_basic_tree" />
		</record>

		<record id="action_etl_connector_basic_view2"
			model="ir.actions.act_window.view">
			<field eval="2" name="sequence" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="etl_connector_basic_form" />
			<field name="act_window_id"
				ref="action_etl_connector_basic_tree" />
		</record>
		
		<menuitem name="Connectors" parent="menu_etl_root" sequence="9"
			action="action_etl_connector_basic_tree" id="etl_connector_all_tree" />

		<record id="etl_connector_localfile_form" model="ir.ui.view">
			<field name="name">etl.connector.localfile.form</field>
			<field name="model">etl.connector</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="New Connector(Localfile)">
					<field name="name" colspan="2" select="1" />
					<field name="type" select="1" on_change="onchange_type(type)" />
					<field name="uri" select="1" required="1"/>
					<field name="bufsize" required="1"/>
				</form>
			</field>
		</record>

		<record id="etl_connector_localfile_tree" model="ir.ui.view">
			<field name="name">etl.connector.localfile.tree</field>
			<field name="model">etl.connector</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Connectors(Localfile)">
					<field name="name" select="1" />
					<field name="type" select="1" />
					<field name="uri" select="1" />
					<field name="bufsize" />
				</tree>
			</field>
		</record>


		<record id="action_etl_connector_localfile_tree"
			model="ir.actions.act_window">
			<field name="name">File Connectors</field>
			<field name="res_model">etl.connector</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('type','=','localfile')]</field>
			<field name="view_id" ref="etl_connector_localfile_tree" />
		</record>

		<record id="action_etl_connector_localfile_view1"
			model="ir.actions.act_window.view">
			<field eval="1" name="sequence" />
			<field name="view_mode">tree</field>
			<field name="view_id" ref="etl_connector_localfile_tree" />
			<field name="act_window_id"
				ref="action_etl_connector_localfile_tree" />
		</record>

		<record id="action_etl_connector_localfile_view2"
			model="ir.actions.act_window.view">
			<field eval="2" name="sequence" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="etl_connector_localfile_form" />
			<field name="act_window_id"
				ref="action_etl_connector_localfile_tree" />
		</record>

		<menuitem name="File Access Control"
			parent="etl_connector_all_tree"
			action="action_etl_connector_localfile_tree"
			id="connector_file_access_control" />


		<record id="etl_connector_openobject_form" model="ir.ui.view">
			<field name="name">etl.connector.openobject.form</field>
			<field name="model">etl.connector</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="New Connector(OpenObject)">
					<field name="name" colspan="2" select="1" />
					<field name="type" select="1" on_change="onchange_type(type)"  />
					<field name="uri" select="1"  required="1"/>
					<field name="db" required="1"/>
					<field name="login"  required="1"/>
					<field name="passwd" password="True"  required="1"/>
					<field name="obj" required="1"/>
					<field name="con_type" required="1"/>
				</form>
			</field>
		</record>

		<record id="etl_connector_openobject_tree" model="ir.ui.view">
			<field name="name">etl.connector.openobject.tree</field>
			<field name="model">etl.connector</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Connectors(OpenObject)">
					<field name="name" select="1" />
					<field name="type" select="1" />
					<field name="uri" select="1" />
					<field name="db" />
					<field name="login" />
					<field name="passwd" password="True" />
					<field name="obj" />
					<field name="con_type" />
				</tree>
			</field>
		</record>


		<record id="action_etl_connector_openobject_tree"
			model="ir.actions.act_window">
			<field name="name">OpenObject Connectors</field>
			<field name="res_model">etl.connector</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('type','=','openobject_connector')]</field>
			<field name="view_id" ref="etl_connector_openobject_tree" />
		</record>

		<record id="action_etl_connector_openobject_view1"
			model="ir.actions.act_window.view">
			<field eval="1" name="sequence" />
			<field name="view_mode">tree</field>
			<field name="view_id" ref="etl_connector_openobject_tree" />
			<field name="act_window_id"
				ref="action_etl_connector_openobject_tree" />
		</record>

		<record id="action_etl_connector_openobject_view2"
			model="ir.actions.act_window.view">
			<field eval="2" name="sequence" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="etl_connector_openobject_form" />
			<field name="act_window_id"
				ref="action_etl_connector_openobject_tree" />
		</record>

		<menuitem name="Open Object Connector"
			action="action_etl_connector_openobject_tree"
			parent="etl_connector_all_tree" id="connector_open_object_connector" />

		<record id="etl_connector_sql_form" model="ir.ui.view">
			<field name="name">etl.connector.sql.form</field>
			<field name="model">etl.connector</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="New Connector(SQL)">
					<field name="name" colspan="2" select="1" />
					<field name="type" select="1" />
					<field name="host" select="1"  required="1"/>
					<field name="port" select="1"  required="1"/>
					<field name="sslmode"  required="1"/>
					<newline />
					<field name="uid" required="1"/>
					<field name="passwd" password="True" required="1"/>
					<field name="con_type" required="1"/>
				</form>
			</field>
		</record>

		<record id="etl_connector_sql_tree" model="ir.ui.view">
			<field name="name">etl.connector.sql.tree</field>
			<field name="model">etl.connector</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Connectors(SQL)">
					<field name="name" select="1" />
					<field name="type" select="1" />
					<field name="host" select="1" />
					<field name="port" select="1" />
					<field name="sslmode" />
					<field name="uid" />
					<field name="passwd" />
					<field name="con_type" />
				</tree>
			</field>
		</record>


		<record id="action_etl_connector_sql_tree"
			model="ir.actions.act_window">
			<field name="name">SQL Connectors</field>
			<field name="res_model">etl.connector</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('type','=','sql_connector')]</field>
			<field name="view_id" ref="etl_connector_sql_tree" />
		</record>

		<record id="action_etl_connector_sql_view1"
			model="ir.actions.act_window.view">
			<field eval="1" name="sequence" />
			<field name="view_mode">tree</field>
			<field name="view_id" ref="etl_connector_sql_tree" />
			<field name="act_window_id"
				ref="action_etl_connector_sql_tree" />
		</record>

		<record id="action_etl_connector_sql_view2"
			model="ir.actions.act_window.view">
			<field eval="2" name="sequence" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="etl_connector_sql_form" />
			<field name="act_window_id"
				ref="action_etl_connector_sql_tree" />
		</record>

		<menuitem name="SQL Connector" parent="etl_connector_all_tree"
			action="action_etl_connector_sql_tree" id="etl_connector_sql" />

	</data>
</openerp>
