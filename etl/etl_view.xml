<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		
	<menuitem name="ETL" id="menu_etl_root" icon="terp-partner" />
		
	<menuitem name="Configuration" parent="menu_etl_root"
			sequence="6" id="menu_etl_configure" />

		<!--  ############  Project definiton	########	 -->

		
	<menuitem name="Project Definitions" parent="menu_etl_configure"
			sequence="5" id="menu_etl_proj_form" />

		<record id="etl_proj_form" model="ir.ui.view">
			<field name="name">view_etl_proj_form</field>
			<field name="model">etl.project</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="etl.project">
					<field name="name" colspan="4" />
					<field name="job_ids" />
				</form>
			</field>
		</record>


		<record id="etl_proj_tree" model="ir.ui.view">
			<field name="name">view_etl_proj_tree</field>
			<field name="model">etl.project</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="etl.project">
					<field name="name" />
					<field name="job_ids" />
				</tree>
			</field>
		</record>

		<record id="action_etl_proj_tree"
			model="ir.actions.act_window">
			<field name="name">ETL Project</field>
			<field name="res_model">etl.project</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="etl_proj_tree" />
		</record>


		<record id="action_etl_proj_form"
			model="ir.actions.act_window">
			<field name="name">ETL Project</field>
			<field name="res_model">etl.project</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
			<field name="view_id" ref="etl_proj_form" />
		</record>

		
	<menuitem name="All Projects" parent="menu_etl_proj_form"
			action="action_etl_proj_tree" id="menu_etl_project_all_form" />

		
	<menuitem name="Current Projects" parent="menu_etl_proj_form"
			action="action_etl_proj_tree" id="menu_etl_project_current_form" />

		
	<menuitem name="New Project" parent="menu_etl_proj_form"
			action="action_etl_proj_form" id="menu_etl_project_new_form" />


		<!--	###########	Channel defination ################# -->

		
	<menuitem name="Channel Defination" parent="menu_etl_configure"
			sequence="6" id="menu_etl_channel_def" />

		<record id="etl_channel_form" model="ir.ui.view">
			<field name="name">view_etl_channel_form</field>
			<field name="model">etl.channel</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="etl.channel">
					<field name="name" colspan="4" />
					<field name="code" />
					<field name="type" />
				</form>
			</field>
		</record>

		<record id="etl_channel_tree" model="ir.ui.view">
			<field name="name">view_etl_channel_tree</field>
			<field name="model">etl.channel</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="etl.channel">
					<field name="name" />
					<field name="code" />
					<field name="type" />
				</tree>
			</field>
		</record>


		<record id="action_etl_channel_all_tree"
			model="ir.actions.act_window">
			<field name="name">ETL Channel</field>
			<field name="res_model">etl.channel</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="etl_channel_tree" />
		</record>

		<record id="action_etl_channel_log_tree"
			model="ir.actions.act_window">
			<field name="name">ETL Channel</field>
			<field name="res_model">etl.channel</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="etl_channel_tree" />
			<field name="domain">[('type','=','logger')]</field>
		</record>

		<record id="action_etl_channel_log_form"
			model="ir.actions.act_window">
			<field name="name">ETL Channel</field>
			<field name="res_model">etl.channel</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
			<field name="view_id" ref="etl_channel_form" />
			<field name="domain">[('type','=','logger')]</field>
		</record>

		<record id="action_etl_channel_trans_tree"
			model="ir.actions.act_window">
			<field name="name">ETL Channel</field>
			<field name="res_model">etl.channel</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="etl_channel_tree" />
			<field name="domain">[('type','=','transition')]</field>
		</record>

		<record id="action_etl_channel_trans_form"
			model="ir.actions.act_window">
			<field name="name">ETL Channel</field>
			<field name="res_model">etl.channel</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
			<field name="view_id" ref="etl_channel_form" />
			<field name="domain">[('type','=','transition')]</field>
		</record>


		
	<menuitem name="Log Channels" parent="menu_etl_channel_def"
			action="action_etl_channel_log_tree" id="menu_etl_log_channel_form" />

		
	<menuitem name="New Log Channel"
			parent="menu_etl_log_channel_form"
			action="action_etl_channel_log_form" id="etl_menu_new_log_channel" />

		
	<menuitem name="Transition Channels"
			action="action_etl_channel_trans_tree" parent="menu_etl_channel_def"
			id="etl_menu_trans_channel" />

		
	<menuitem name="New Transition Channel"
			parent="etl_menu_trans_channel"
			action="action_etl_channel_trans_form"
			id="etl_menu_new_trans_channel" />


