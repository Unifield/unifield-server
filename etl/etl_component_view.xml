<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record id="view_etl_component_type_form" model="ir.ui.view">
			<field name="name">view.etl.component.type.form</field>
			<field name="model">etl.component.type</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Component Type">
					<field name="name" select="1" colspan="4" />
					<field name="code" />
					<separator string="Arguments" colspan="4" />
					<field name="args" nolabel="1" colspan="4" />
				</form>
			</field>
		</record>


		<record id="view_etl_component_type_tree" model="ir.ui.view">
			<field name="name">view.etl.component.type.tree</field>
			<field name="model">etl.component.type</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Component Type">
					<field name="name" />
					<field name="code" />
				</tree>
			</field>
		</record>


		<record id="action_etl_component_type_tree"
			model="ir.actions.act_window">
			<field name="name">ETL Component</field>
			<field name="res_model">etl.component.type</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="view_etl_component_type_tree" />
		</record>


		<menuitem name="Component types" parent="menu_etl_configure"
			sequence="10" id="menu_etl_component_types" />

		<menuitem name="Component types"
			parent="menu_etl_component_types"
			action="action_etl_component_type_tree" sequence="10"
			id="menu_action_etl_component_types" />


		<record id="etl_component_form" model="ir.ui.view">
			<field name="name">view.etl.component.form</field>
			<field name="model">etl.component</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Component">
					<field name="name" select="1" />
					<field name="type" />
					<separator string="Incomming Transitions"
						colspan="4" />
					<field name="trans_in_ids" nolabel="1" colspan="4"
						readonly="1" />
					<separator string="Outgoing Transitions"
						colspan="4" />
					<field name="trans_out_ids" nolabel="1" colspan="4"
						readonly="1" />
				</form>
			</field>
		</record>


		<record id="etl_component_tree" model="ir.ui.view">
			<field name="name">view.etl.component.tree</field>
			<field name="model">etl.component</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Components">
					<field name="name" />
					<field name="type" />
					<field name="trans_in_ids" />
					<field name="trans_out_ids" />
				</tree>
			</field>
		</record>


		<record id="action_etl_component_tree"
			model="ir.actions.act_window">
			<field name="name">ETL Component</field>
			<field name="res_model">etl.component</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="etl_component_tree" />
		</record>

		<record id="etl_component_csv_in_form" model="ir.ui.view">
			<field name="name">view.etl.component.csv.in.form</field>
			<field name="model">etl.component</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="CSV-IN Components">
					<field name="name" select="1" />
					<field name="type" />
					<field name="connector_id" />
					<field name="transformer_id" />
					<field name="row_limit" />
					<field name="csv_params" />
					<separator string="Incomming Transitions"
						colspan="4" />
					<field name="trans_in_ids" nolabel="1" colspan="4"
						readonly="1" />
					<separator string="Outgoing Transitions"
						colspan="4" />
					<field name="trans_out_ids" nolabel="1" colspan="4"
						readonly="1" />
				</form>
			</field>
		</record>

		<record id="action_etl_component_csv_in_tree"
			model="ir.actions.act_window">
			<field name="res_model">etl.component</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('type.name','=','input.csv_in')]</field>
		</record>
		
		<record id="action_etl_component_csv_in_from"
			model="ir.actions.act_window">
			<field name="res_model">etl.component</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
			<field name="domain">[('type.name','=','input.csv_in')]</field>
			<field name="view_id" ref="etl_component_csv_in_form"/>
		</record>
		
		 <record id="action_etl_component_csv_in_view1" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="etl_component_tree"/>
            <field name="act_window_id" ref="action_etl_component_csv_in_tree"/>
        </record>
        <record id="action_etl_component_csv_in_view2" model="ir.actions.act_window.view">
