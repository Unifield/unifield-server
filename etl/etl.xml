		<record id="view_etl_job_form" model="ir.ui.view">
			<field name="name">view_etl_job_form</field>
			<field name="model">etl.job</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="etl_job">
					<notebook colspan="4">
						<page string="Job Definition">
							<field name="name" select="1" />
							<field name="project_id" />
							<field name="user_id" />
							<field name="author" />
							<field name="is_start" />

							<newline />
							<separator string="Notes" colspan="4" />
							<field name="notes" colspan="4" nolabel="1" />
							<newline />
							<separator string="Status" colspan="4" />
							<field name="state" />
							<group col="2" colspan="2">
								<button name="open_job"
									string="Open Job" type="object" />
								<button name="launch_process"
									string="Launch a Process" type="object" />
							</group>
						</page>
						<page string="Job Components">
							<field name="component_ids" nolabel="1"
								colspan="4" widget="one2many_list">
								<tree string="Components">
									<field name="name" select="1" />
									<field name="job_id" />
								</tree>
								<form string="Components">
									<field name="name" select="1" />
									<field name="job_id" />
								</form>
							</field>

						</page>
						<page string="Statistics">
							<newline />
							<separator string="Processes" colspan="4" />
							<field name="running_process" />
							<field name="total_process" />
						</page>
					</notebook>
				</form>
			</field>
		</record>


