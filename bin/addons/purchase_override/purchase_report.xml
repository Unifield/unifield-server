<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <report
            auto="False"
            id="report_msf_purchase_order"
            model="purchase.order"
            name="msf.purchase.order"
            rml="purchase_override/report/purchase_order.rml"
            string="Purchase Order"
            view_ids="purchase.purchase_order_form,purchase.purchase_order_tree"
        />

        <report
            report_type="webkit"
            header="False"
            auto="False"
            menu="False"
            id="report_purchase_order_merged"
            model="purchase.order"
            name="purchase.order.merged"
            rml="purchase_override/report/merged_order.rml"
            string="Purchase Order (Merged)"
        />

        <record id="action_purchase_order_merged_export" model="ir.actions.server">
            <field name="name">Purchase Order (Merged)</field>
            <field name="model_id" ref="model_wizard_purchase_order_merged_export" />
            <field name="state">code</field>
            <field name="code">action = obj.print_report_pdf(context=context)</field>
        </record>

        <record id="ir_print_purchase_order_merged_export" model="ir.values">
            <field name="key2">client_print_multi</field>
            <field name="model">purchase.order</field>
            <field name="name">Purchase Order (Merged)</field>
            <field name="view_ids" ref="purchase.purchase_order_form,purchase.purchase_order_tree" />
            <field eval="'ir.actions.server,%d'%action_purchase_order_merged_export" name="value" />
            <field eval="True" name="object" />
            <field name="sequence" eval="2"/>
        </record>
    </data>
</openerp>
