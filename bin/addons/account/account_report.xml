<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <report auto="False" id="account_general_ledger" menu="False" model="account.account" name="account.general.ledger" rml="account/report/account_general_ledger.rml" string="General Ledger" target_filename="General Ledger_%(instance_code)s%(year)s%(month)s%(day)s" />
        <report auto="False" id="account_general_ledger_landscape" menu="False" model="account.account" name="account.general.ledger_landscape" rml="account/report/account_general_ledger_landscape.rml" string="General Ledger" target_filename="General Ledger_%(instance_code)s%(year)s%(month)s%(day)s"/>
