<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <record id="auto_export_fnct_purchase_order" model="automated.export.function">
            <field name="name">Export validated Purchase Order</field>
            <field name="model_id" model="ir.model" search="[('model', '=', 'purchase.order')]" />
            <field name="method_to_call">auto_export_validated_purchase_order</field>
            <field name="multiple" eval="True" />
        </record>
        <record id="auto_export_fnct_oca_finance_vi" model="automated.export.function">
            <field name="name">Export to HQ system (OCA) - Not yet exported</field>
            <field name="model_id" model="ir.model" search="[('model', '=', 'wizard.hq.report.oca')]" />
            <field name="method_to_call">auto_export_vi</field>
            <field name="disable_cron" eval="True" />
        </record>
        <record id="auto_export_fnct_ocp_finance_vi" model="automated.export.function">
            <field name="name">Export to HQ system (OCP)</field>
            <field name="model_id" model="ir.model" search="[('model', '=', 'ocp.export.wizard')]" />
            <field name="method_to_call">auto_export_vi</field>
            <field name="disable_cron" eval="True" />
        </record>
    </data>
</openerp>
