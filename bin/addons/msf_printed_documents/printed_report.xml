<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <report auto="False" id="sale.report_sale_order" model="sale.order" name="msf.sale.order" rml="msf_printed_documents/report/sale_order.rml" string="Field Order" view_ids="sale.view_order_form,sale.view_order_tree"/>

        <report auto="False" id="sale.report_reception_in" model="stock.picking" name="msf.report_reception_in" rml="msf_printed_documents/report/report_reception.rml" string="Reception" view_ids="stock.view_picking_in_tree,stock.view_picking_in_form" target_filename='${eval(_get_stock_picking_report_name)}'/>

        <report auto="False" id="sale.consumption_report" model="real.average.consumption" name="msf.consumption_report" rml="msf_printed_documents/report/consumption_report.rml" string="Consumption Report" target_filename='${eval(_get_report_name)}' />

        <report auto="False" id="sale.product_list_report" model="product.list" name="msf.product_list" rml="msf_printed_documents/report/product_list.rml" string="Product List / Sub-List" />

        <!-- PRODUCT LABELS -->
        <report
            id="product_labels_batch"
            string="Product Labels"
            model="stock.production.lot"
            name="product_labels_batch"
            rml="msf_printed_documents/report/report_product_labels.rml"
            header="False"
            auto="False"
            menu="True"
        />

        <report
            id="product_labels_product"
            string="Product Labels"
            model="product.product"
            name="product_labels_product"
            rml="msf_printed_documents/report/report_product_labels.rml"
            header="False"
            auto="False"
            menu="True"
            sequence="150"
            view_ids="product.product_product_tree_view,product.product_normal_form_view"
        />

        <report
            id="product_labels_stock_card"
            string="Product Labels"
            model="stock.card.wizard"
            name="product_labels_stock_card"
            rml="msf_printed_documents/report/report_product_labels.rml"
            header="False"
            auto="False"
            menu="True"
        />
    </data>
</openerp>
