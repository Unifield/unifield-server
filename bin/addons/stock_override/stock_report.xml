    <data>

        <report
            auto="False"
            id="stock_card_report"
            model="stock.card.wizard"
            name="stock.card.report"
            header="False"
            rml="addons/stock_override/report/stock_card_report.rml"
            string="Stock card" />
            
        <!-- STOCK CARD -->
		<report
            id="stock_card_report_xls"
            string="Stock card Excel Export"
            model="stock.card.wizard"
            name="stock.card.report.xls"
