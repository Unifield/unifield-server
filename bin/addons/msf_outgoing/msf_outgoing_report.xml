<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <!-- delete packing list original report -->
    	
                
        <report auto="False" 
                id="report_invoice"
                model="shipment"
                name="invoice"
                header="False"
                rml="addons/msf_outgoing/report/invoice.rml"
                string="Invoice" />
                
        <report auto="False" 
                id="report_certificate_free_gift" 
                model="shipment" 
                name="certificate.free.gift"
                header="False"
                rml="addons/msf_outgoing/report/certificate_free_gift.rml"
                string="Free Gift Certificate" />
                
        <report auto="False" 
                id="report_packing_list" 
                model="shipment" 
                name="packing.list"
                header="False"
                rml="addons/msf_outgoing/report/packing_list.rml"
                string="Packing List" />

        <report auto="False"
                id="report_packing_list_xls"
                model="shipment"
                name="packing.list.xls"
                header="False"
                report_type="webkit"
                file="msf_outgoing/report/packing_list_xls.mako"
                string="Packing List (xls)" />
                
                
        <report auto="False" 
                id="picking_ticket" 
                model="stock.picking" 
                name="picking.ticket"
                header="False"
                rml="addons/msf_outgoing/report/picking_ticket.rml"
                string="Picking Ticket"
                update_title="1"
                view_ids="msf_outgoing.view_picking_ticket_tree,msf_outgoing.view_picking_ticket_form"
                />

        <report auto="False"
                id="empty_picking_ticket"
                model="stock.picking"
                name="empty.picking.ticket"
                header="False"
                rml="addons/msf_outgoing/report/empty_picking_ticket.rml"
                string="Pre-Packing List"
                view_ids="msf_outgoing.view_ppl_tree,msf_outgoing.view_ppl_form"
                />

        <report id="pre_packing_excel_export"
                name="pre.packing.excel.export"
                model="stock.picking"
                string="Pre-Packing Excel Export"
                file="msf_outgoing/report/pre_packing_excel_report_xls.mako"
                report_type="webkit"
                header="False"
                menu="False"
                auto="False" />
                
        <report auto="False" 
                id="labels" 
                model="stock.picking" 
                name="labels"
                header="False"
                rml="addons/msf_outgoing/report/labels.rml"
                string="Labels"
                view_ids="msf_outgoing.view_ppl_tree,msf_outgoing.view_ppl_form"
                />
    
