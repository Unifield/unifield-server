<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>
        <!-- delete packing list original report -->
        <report auto="False"
                id="report_freight_manifest"
                model="shipment"
                name="freight_manifest"
                header="False"
                rml="addons/msf_outgoing/report/freight_manifest.rml"
                string="Freight Manifest"
                sequence="1" />

        <report auto="False"
                id="report_freight_manifest_xls"
                model="shipment"
                name="freight_manifest_xls"
                header="False"
                rml="msf_outgoing/report/freight_manifest_xls.mako"
                report_type="webkit"
                string="Freight Manifest (xls)"
                sequence="2" />

        <report auto="False" 
                id="report_invoice"
                model="shipment"
                name="invoice"
                header="False"
                rml="addons/msf_outgoing/report/invoice.rml"
                string="Invoice"
                sequence="3" />

        <report auto="False"
                id="report_shipment_donation_certificate"
                model="shipment"
                name="shipment.donation.certificate"
                header="False"
                rml="addons/msf_outgoing/report/shipment_donation_certificate.rml"
                string="Donation Certificate"
                sequence="4" />

        <report auto="False"
                id="report_certificate_free_gift" 
                model="shipment" 
                name="certificate.free.gift"
                header="False"
                rml="addons/msf_outgoing/report/certificate_free_gift.rml"
                string="Free Gift Certificate"
                sequence="5" />

        <report auto="False" 
                id="report_packing_list" 
                model="shipment" 
                name="packing.list"
                header="False"
                rml="addons/msf_outgoing/report/packing_list.rml"
                string="Packing List"
                sequence="6" />

        <report auto="False"
                id="report_packing_list_xls"
                model="shipment"
                name="packing.list.xls"
                header="False"
                report_type="webkit"
                file="msf_outgoing/report/packing_list_xls.mako"
                string="Packing List (xls)"
                sequence="7" />

        <report
            id="report_dispatched_packing_list_xls"
            model="shipment"
            name="dispatched.packing.list.xls"
            file="msf_outgoing/report/dispatched_packing_list_xls.mako"
            report_type="webkit"
            header="False"
            menu="False"
            auto="False"
            string="Dispatched Packing List (xls)" />

        <!-- The rest is in addons/stock/wizard/loan_certificate_wizard.xml -->
        <act_window id="action_ship_loan_certificate_wizard"
            name="Loan Certificate Export"
            view_ids="view_shipment_tree,view_shipment_form"
            res_model="ship.loan.certificate.wizard"
            src_model="shipment"
            view_mode='form'
            view_type='form'
            key2="client_print_multi"
            context="{'active_id': active_id}"
            target='new' />


        <report auto="False" 
                id="picking_ticket" 
                model="stock.picking" 
                name="picking.ticket"
                header="False"
                rml="addons/msf_outgoing/report/picking_ticket.rml"
                string="Picking Ticket"
                view_ids="msf_outgoing.view_picking_ticket_tree,msf_outgoing.view_picking_ticket_form"
                />

        <report auto="False"
                id="empty_picking_ticket"
                model="stock.picking"
                name="empty.picking.ticket"
                header="False"
                rml="addons/msf_outgoing/report/empty_picking_ticket.rml"
                string="Pre-Packing List"
                view_ids="msf_outgoing.view_ppl_tree,msf_outgoing.view_ppl_form"
                />

        <report id="pre_packing_excel_export"
                name="pre.packing.excel.export"
                model="stock.picking"
                string="Pre-Packing Excel Export"
                file="msf_outgoing/report/pre_packing_excel_report_xls.mako"
                report_type="webkit"
                header="False"
                menu="False"
                auto="False" />

        <report auto="False" 
                id="labels" 
                model="stock.picking" 
                name="labels"
                header="False"
                rml="addons/msf_outgoing/report/labels.rml"
                string="Labels"
                view_ids="msf_outgoing.view_ppl_tree,msf_outgoing.view_ppl_form"
                />

    </data>
</openerp>
