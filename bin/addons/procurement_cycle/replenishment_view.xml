<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data>

        <record id="replenishment_location_config_form" model="ir.ui.view">
            <field name="name">replenishment.location.config.form</field>
            <field name="model">replenishment.location.config</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Location Configuration">
                    <field name="name" />
                    <field name="description" />
                    <field name="main_instance" />
                    <group colspan="2" col="4">
                        <field name="synched" />
                        <field name="active" />
                    </group>
                    <group colspan="2" col="2">
                        <separator string="Local Locations" colspan="2" />
                        <field name="local_location_ids" nolabel="1">
                        <tree hide_new_button="0" hide_edit_button="1">
                                <field name="complete_name" />
                                <field name="usage" />
                                <field name="location_category" />
                            </tree>
                        </field>
                    </group>
                    <group colspan="2" col="2" attrs="{'invisible': [('synched', '=', False)]}">
                        <separator string="Project Locations" colspan="2" />
                        <field name="remote_location_ids" nolabel="1">
                            <tree hide_edit_button="1">
                                <field name="full_name" />
                            </tree>
                        </field>
                    </group>
                </form>
            </field>
        </record>

        <record id="replenishment_location_config_tree" model="ir.ui.view">
            <field name="name">replenishment.location.config.tree</field>
            <field name="model">replenishment.location.config</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Location Configuration">
                    <field name="name" />
                    <field name="description" />
                    <field name="synched" />
                    <field name="main_instance" />
                </tree>
            </field>
        </record>

        <record id="replenishment_location_config_search" model="ir.ui.view">
            <field name="name">replenishment.location.config.search</field>
            <field name="model">replenishment.location.config</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Location Configuration">
                    <field name="name" />
                    <field name="description" />
                    <field name="synched" />
                    <field name="main_instance" />
                </search>
            </field>
        </record>

        <record id="replenishment_location_config_action" model="ir.actions.act_window">
            <field name="name">Location Configuration</field>
            <field name="res_model">replenishment.location.config</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="replenishment_location_config_menu"
            action="replenishment_location_config_action"
            sequence="10"
            parent="stock_schedule.stock_automatic_procurements_menu" />


        <record id="replenishment_review_config_form" model="ir.ui.view">
            <field name="name">replenishment.review.config.form</field>
            <field name="model">replenishment.location.config</field>
            <field name="type">form</field>
            <field name="priority" eval="50" />
            <field name="arch" type="xml">
                <form string="Inventory Review Configuration" hide_new_button="1" hide_duplicate_button="1" hide_delete_button="1">
                    <field name="name" readonly="1" />
                    <field name="description" readonly="1" />
                    <field name="frequence_name" />
                    <newline />
                    <button name="choose_change_frequence" type="object" string="Change/Choose Frequency" icon="terp-go-today" colspan="2" />
                    <field name="review_active"  required="1" />
                    <field name="projected_view" required="1" />
                    <field name="rr_amc" required="1" />
                    <field name="sleeping" required="1" />
                    <field name="time_unit" required="1" />
                </form>
            </field>
        </record>

        <record id="replenishment_review_config_tree" model="ir.ui.view">
            <field name="name">replenishment.review.config.tree</field>
            <field name="model">replenishment.location.config</field>
            <field name="type">tree</field>
            <field name="priority" eval="50" />
            <field name="arch" type="xml">
                <tree string="Inventory Review Configuration" hide_new_button="1" hide_delete_button="1">
                    <field name="name" />
                    <field name="description" />
                    <field name="review_active" />
                </tree>
            </field>
        </record>

        <record id="replenishment_review_config_search" model="ir.ui.view">
            <field name="name">replenishment.review.config.search</field>
            <field name="model">replenishment.location.config</field>
            <field name="type">search</field>
            <field name="priority" eval="50" />
            <field name="arch" type="xml">
                <search string="Inventory Review Configuration">
                    <field name="name" />
                    <field name="description" />
                    <field name="review_active" />
                    <field name="active"  />
                </search>
            </field>
        </record>

        <record id="replenishment_review_config_action" model="ir.actions.act_window">
            <field name="name">Inventory Review Configuration</field>
            <field name="res_model">replenishment.location.config</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="replenishment_review_config_search" />
        </record>
        <record id="replenishment_review_config_action_tree" model="ir.actions.act_window.view">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="replenishment_review_config_tree"/>
            <field name="act_window_id" ref="replenishment_review_config_action"/>
        </record>
        <record id="replenishment_review_config_action_form" model="ir.actions.act_window.view">
            <field eval="2" name="sequence"/>
            <field name="view_mode">form</field>
            <field name="view_id" ref="replenishment_review_config_form"/>
            <field name="act_window_id" ref="replenishment_review_config_action"/>
        </record>

        <menuitem id="replenishment_review_config_menu"
            action="replenishment_review_config_action"
            sequence="20"
            parent="stock_schedule.stock_automatic_procurements_menu" />


        <record id="replenishment_segment_form" model="ir.ui.view">
            <field name="name">replenishment.segment.form</field>
            <field name="model">replenishment.segment</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="Segment">
                    <group colspan="2" col="2">
                    <field name="name_seg" />
                    <field name="description_seg" />
                    <field name="location_config_id" />
                    <separator string="Location Config"/>
                    <group colspan="2" col="4">
                        <field name="name" readonly="1" />
                        <field name="description" readonly="1" />
                        <field name="main_instance" readonly="1" />
                        <field name="rr_amc" readonly="1" />
                        <field name="local_location_ids" colspan="2" nolabel="1" readonly="1">
                            <tree>
                                <field name="complete_name" />
                            </tree>
                        </field>
                        <field name="remote_location_ids" colspan="2" nolabel="1" readonly="1">
                            <tree>
                                <field name="full_name" string="Other project Locations"/>
                            </tree>
                        </field>
                    </group>
                    <field name="rule" />
                    <field name="rule_alert" />
                    <field name="ir_requesting_location" />
                    <field name="product_list_id" />
                    <field name="state" />
                    </group>
                    <group colspan="2" col="2">
                    <field name="order_creation_lt" on_change="on_change_lt(order_creation_lt, order_validation_lt, supplier_lt, handling_lt)" />
                    <field name="order_validation_lt" on_change="on_change_lt(order_creation_lt, order_validation_lt, supplier_lt, handling_lt)" />
                    <field name="internal_lt" />
                    <field name="supplier_lt" on_change="on_change_lt(order_creation_lt, order_validation_lt, supplier_lt, handling_lt)" />
                    <field name="handling_lt" on_change="on_change_lt(order_creation_lt, order_validation_lt, supplier_lt, handling_lt)" />
                    <field name="external_lt" />
                    <field name="total_lt" />
                    <group colspan="2" col="4" attrs="{'invisible': [('rule', '!=', 'cycle')]}">
                        <field name="order_coverage" />
                        <field name="safety_stock" />
                        <field name="previous_order_rrd" />
                        <field name="date_preparing" />
                        <field name="date_next_order_validated" />
                        <field name="date_next_order_received" />
                    </group>
                    </group>
                    <button name="trigger_compute" string="Trigger Compute" type="object" />
                    <field name="line_ids" colspan="4" nolabel="1" default_get="rule=rule">
                        <tree editable="top" editable_style="new">
                            <field name="product_id" />
                            <field name="product_description" displayon="noteditable"/>
                            <field name="uom_id" invisible="1" />
                            <field name="in_main_list" />
                            <field name="status" />
                            <field name="min_qty" />
                            <field name="max_qty" />
                            <field name="auto_qty" invisible="context.get('rule')!='auto'"/>
                            <field name="buffer_qty" invisible="context.get('rule')!='cycle'"/>
                            <field name="real_stock" />
                            <field name="rr_amc" />
                            <field name="rr_fmc_1" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_1" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_1" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'" />
                            <field name="rr_fmc_2" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_2" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_2" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'" />
                            <field name="rr_fmc_3" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_3" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_3" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_4" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_4" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_4" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_5" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_5" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_5" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_6" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_6" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_6" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_7" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_7" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_7" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_8" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_8" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_8" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_9" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_9" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_9" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_10" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_10" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_10" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_11" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_11" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_11" invisible="context.get('rule')!='cycle'"/>
                            <separator invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_12" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_from_12" invisible="context.get('rule')!='cycle'"/>
                            <field name="rr_fmc_to_12" invisible="context.get('rule')!='cycle'"/>
                        </tree>
                    </field>
                </form>
            </field>
        </record>

        <record id="replenishment_segment_tree" model="ir.ui.view">
            <field name="name">replenishment.segment.tree</field>
            <field name="model">replenishment.segment</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="Segment">
                    <field name="name_seg" />
                    <field name="description_seg" />
                    <field name="line_ids" />
                    <field name="location_config_id" />
                    <field name="rule" />
                    <field name="product_list_id" />
                    <field name="date_preparing" />
                    <field name="date_next_order_validated" />
                    <field name="state" />
                </tree>
            </field>
        </record>

        <record id="replenishment_segment_search" model="ir.ui.view">
            <field name="name">replenishment.segment.search</field>
            <field name="model">replenishment.segment</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Segment">
                    <field name="name_seg" />
                    <field name="description_seg" />
                    <field name="line_ids" />
                    <field name="location_config_id" />
                    <field name="rule" />
                    <field name="product_list_id" />
                    <field name="state" />
                </search>
            </field>
        </record>

        <record id="replenishment_segment_action" model="ir.actions.act_window">
            <field name="name">Segment</field>
            <field name="res_model">replenishment.segment</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="replenishment_segment_menu"
            action="replenishment_segment_action"
            sequence="40"
            parent="stock_schedule.stock_automatic_procurements_menu" />
    </data>
</openerp>
