<?xml version="1.0" encoding="utf-8" ?>
<openerp>
    <data noupdate="1">
        <!-- Macroprocesses -->
        <record id="macroprocess_international_transport_import" model="transport.macroprocess">
            <field name="name">International Transport &amp; Import process</field>
            <field name="active">True</field>
            <field name="transport_management">in</field>
        </record>

        <record id="macroprocess_international_transport_transit" model="transport.macroprocess">
            <field name="name">International Transport &amp; Transit process</field>
            <field name="active">True</field>
            <field name="transport_management">both</field>
        </record>

        <record id="macroprocess_international_transport_temp" model="transport.macroprocess">
            <field name="name">International Transport &amp; Temporary Admission process</field>
            <field name="active">True</field>
            <field name="transport_management">both</field>
        </record>

        <record id="macroprocess_international_transport_bonded" model="transport.macroprocess">
            <field name="name">International Transport &amp; Bonded Warehousing</field>
            <field name="active">True</field>
            <field name="transport_management">both</field>
        </record>

        <record id="macroprocess_bonded_domestic" model="transport.macroprocess">
            <field name="name">Bonded Ex-Warehousing &amp; Domestic Transport</field>
            <field name="active">True</field>
            <field name="transport_management">out</field>
        </record>

        <record id="macroprocess_bonded_international" model="transport.macroprocess">
            <field name="name">Bonded Ex-Warehousing &amp; International Transport</field>
            <field name="active">True</field>
            <field name="transport_management">out</field>
        </record>

        <record id="macroprocess_reexport_international" model="transport.macroprocess">
            <field name="name">Re-Export &amp; International Transport</field>
            <field name="active">True</field>
            <field name="transport_management">out</field>
        </record>

        <record id="macroprocess_export_international" model="transport.macroprocess">
            <field name="name">Export &amp; International Transport</field>
            <field name="active">True</field>
            <field name="transport_management">out</field>
        </record>

        <record id="macroprocess_motor_vehicle" model="transport.macroprocess">
            <field name="name">Motor vehicle Process</field>
            <field name="active">True</field>
            <field name="transport_management">out</field>
        </record>

        <record id="macroprocess_domestic" model="transport.macroprocess">
            <field name="name">Domestic Transport</field>
            <field name="active">True</field>
            <field name="transport_management">out</field>
        </record>

        <!-- Steps -->
        <record id="step_export_cargo_req" model="transport.step">
            <field name="name">Export Cargo request</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_export_international')])]"/>
        </record>

        <record id="step_reexport_cargo_req" model="transport.step">
            <field name="name">Re-Export Cargo request</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international')])]"/>
        </record>

        <record id="step_req_dispose_msf_fleet" model="transport.step">
            <field name="name">Request to Dispose of MSF Fleet (Re-Export)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_motor_vehicle')])]"/>
        </record>

        <record id="step_domestic_transport_req" model="transport.step">
            <field name="name">Domestic Transport request</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_domestic')])]"/>
        </record>

        <record id="step_cargo_notif" model="transport.step">
            <field name="name">Cargo Notification (Ready to Ship)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_transit'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded'), ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international')])]"/>
        </record>

        <record id="step_notif_conf_rts" model="transport.step">
            <field name="name">Notification Confirmation of RTS (Ready to Ship)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_motor_vehicle')])]"/>
        </record>

        <record id="step_prearrival_proc" model="transport.step">
            <field name="name">Pre-Arrival processing (Preclearance)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_transit'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded')])]"/>
        </record>

        <record id="step_preship_preclear" model="transport.step">
            <field name="name">Pre-shipment / Preclearance (Receiving Country)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international')])]"/>
        </record>

        <record id="step_predispatch_proc" model="transport.step">
            <field name="name">Pre-Dispatch processing (Greenlight &amp; Confirmation of shipment)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_domestic')])]"/>
        </record>

        <record id="step_greenlight_shipper" model="transport.step">
            <field name="name">Greenlight to Shipper</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_transit'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded')])]"/>
        </record>

        <record id="step_greenlight_shipper_coor" model="transport.step">
            <field name="name">Greenlight to Shipper (Coordination)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_domestic')])]"/>
        </record>

        <record id="step_greenlight_consignee" model="transport.step">
            <field name="name">Greenlight from Consignee (Receiving Country)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international'), ref('macroprocess_motor_vehicle')])]"/>
        </record>

        <record id="step_proc_deregistration_bond_cancel" model="transport.step">
            <field name="name">Processing De-Registration &amp; Bond Cancellation</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_motor_vehicle')])]"/>
        </record>

        <record id="step_ship_conf" model="transport.step">
            <field name="name">Shipment confirmation</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded')])]"/>
        </record>

        <record id="step_transit_ship_conf" model="transport.step">
            <field name="name">Transit Shipment confirmation</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_transit')])]"/>
        </record>

        <record id="step_reexport_ship_conf" model="transport.step">
            <field name="name">Re-Export Shipment confirmation &amp; Dispatch (Booking LT (in days), ETD origin port, ETA destination port, ATD origin port)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international')])]"/>
        </record>

        <record id="step_ship_conf_bond_invoice" model="transport.step">
            <field name="name">Shipment confirmation &amp; Bond Invoice Processing (Booking LT (in days), ETD origin port, ETA destination port, ATD origin port)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_motor_vehicle')])]"/>
        </record>

        <record id="step_ship_dispatch_customs_exit" model="transport.step">
            <field name="name">Shipment Dispatch &amp; Customs Exit Point Processing</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_motor_vehicle')])]"/>
        </record>

        <record id="step_cargo_arrival" model="transport.step">
            <field name="name">Cargo arrival processing (ATA destination port)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded')])]"/>
        </record>

        <record id="step_transit_cargo_arrival" model="transport.step">
            <field name="name">Transit Cargo arrival processing (ATA transit port)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_transit')])]"/>
        </record>

        <record id="step_cargo_customs_bonded" model="transport.step">
            <field name="name">Cargo Customs Bonded warehouse processing</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_bonded')])]"/>
        </record>

        <record id="step_cargo_customs" model="transport.step">
            <field name="name">Cargo Customs release and onward preparation</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import')])]"/>
        </record>

        <record id="step_cargo_customs_temp" model="transport.step">
            <field name="name">Cargo Customs temporary release and onward preparation</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_temp')])]"/>
        </record>

        <record id="step_imp_cargo_req" model="transport.step">
            <field name="name">Import Cargo request</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_domestic')])]"/>
        </record>

        <record id="step_customs_bonded_ex_wh" model="transport.step">
            <field name="name">Customs Bonded Ex-Warehousing processing</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_domestic')])]"/>
        </record>

        <record id="step_transport_freight_req" model="transport.step">
            <field name="name">Transport / Freight request</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_domestic')])]"/>
        </record>

        <record id="step_bonded_wh_invoice" model="transport.step">
            <field name="name">Bonded Warehouse Invoice Processing</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_domestic'), ref('macroprocess_bonded_international')])]"/>
        </record>

        <record id="step_transport_reception" model="transport.step">
            <field name="name">Transport reception &amp; Claims</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded'), ref('macroprocess_bonded_domestic'), ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international'), ref('macroprocess_motor_vehicle'), ref('macroprocess_domestic')])]"/>
        </record>

        <record id="step_transport_transit_reception" model="transport.step">
            <field name="name">Transport transit reception &amp; Claims (transit country)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_transit')])]"/>
        </record>

        <record id="step_transport_onward_transit_proc" model="transport.step">
            <field name="name">Transport Onward transit processing (onward Transport preparation to Destination country)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_transit')])]"/>
        </record>

        <record id="step_transport_onward_transit_greenlight" model="transport.step">
            <field name="name">Transport Onward transit greenlight (Cargo ETA Confirmation)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_transit')])]"/>
        </record>

        <record id="step_transport_customs_invoice" model="transport.step">
            <field name="name">Transport &amp; Customs Invoice processing</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded'), ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international'), ref('macroprocess_motor_vehicle')])]"/>
        </record>

        <record id="step_domestic_transport_invoice" model="transport.step">
            <field name="name">Domestic Transport Invoice processing</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_bonded_domestic'), ref('macroprocess_domestic')])]"/>
        </record>

        <record id="step_goods_reception" model="transport.step">
            <field name="name">Goods reception &amp; Claims</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_transit'), ref('macroprocess_international_transport_temp'), ref('macroprocess_bonded_domestic'), ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international'), ref('macroprocess_motor_vehicle'), ref('macroprocess_domestic')])]"/>
        </record>

        <record id="step_goods_reception" model="transport.step">
            <field name="name">Goods reception &amp; Claims (at customs bonded warehouse)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_bonded')])]"/>
        </record>

        <record id="step_transport_reception_dest" model="transport.step">
            <field name="name">Transport reception &amp; Claims (Destination)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_transit')])]"/>
        </record>

        <record id="step_transport_transit_invoice" model="transport.step">
            <field name="name">Transport Transit Invoice processing</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_transit')])]"/>
        </record>

        <record id="step_doc_archiving" model="transport.step">
            <field name="name">Documentation Archiving</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_transit'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded'), ref('macroprocess_bonded_domestic'), ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international'), ref('macroprocess_motor_vehicle'), ref('macroprocess_domestic')])]"/>
        </record>

        <record id="step_perf_indicator" model="transport.step">
            <field name="name">Key Performance Indicators (Incident monitoring and control)</field>
            <field name="is_active">True</field>
            <field name="macroprocess_ids" eval="[(6, 0, [ref('macroprocess_international_transport_import'), ref('macroprocess_international_transport_transit'), ref('macroprocess_international_transport_temp'), ref('macroprocess_international_transport_bonded'), ref('macroprocess_bonded_domestic'), ref('macroprocess_bonded_international'), ref('macroprocess_reexport_international'), ref('macroprocess_export_international'), ref('macroprocess_motor_vehicle'), ref('macroprocess_domestic')])]"/>
        </record>
    </data>

    <data noupdate="0">

        <record id="stock.incoterm_EXW" model="stock.incoterms">
          <field name="name">Ex Works (named place of delivery)</field>
          <field name="code">EXW</field>
        </record>

        <record id="stock.incoterm_FCA" model="stock.incoterms">
          <field name="name">Free Carrier (named place of delivery)</field>
          <field name="code">FCA</field>
        </record>

        <record id="stock.incoterm_CPT" model="stock.incoterms">
          <field name="name">Carriage Paid To (named place of destination)</field>
          <field name="code">CPT</field>
        </record>

        <record id="stock.incoterm_CIP" model="stock.incoterms">
          <field name="name">Carriage and Insurance Paid to (named place of destination)</field>
          <field name="code">CIP</field>
        </record>

        <record id="stock.incoterm_DAT" model="stock.incoterms">
          <field name="name">Delivered at Terminal (named terminal at port or place of destination)</field>
          <field name="code">DAT</field>
        </record>

        <record id="stock.incoterm_DAP" model="stock.incoterms">
          <field name="name">Delivered at Place (named place of destination)</field>
          <field name="code">DAP</field>
        </record>

        <record id="stock.incoterm_DDU" model="stock.incoterms">
          <field name="name">Delivered Duty Paid (named place of destination)</field>
          <field name="code">DDP</field>
        </record>

        <record id="stock.incoterm_FOB" model="stock.incoterms">
          <field name="name">Free On Board (named place of destination)</field>
          <field name="code">FOB</field>
        </record>

        <record id="stock.incoterm_CFR" model="stock.incoterms">
          <field name="name">Cost And Freight (named place of destination)</field>
          <field name="code">CFR</field>
        </record>

        <record id="stock.incoterm_CIF" model="stock.incoterms">
          <field name="name">Cost, Insurance and Freight (named place of destination)</field>
          <field name="code">CIF</field>
        </record>

        <record id="seq_type_transport_in" model="ir.sequence.type">
            <field name="name">Inbound Transport</field>
            <field name="code">transport.order.in</field>
        </record>

        <record id="seq_transport_in" model="ir.sequence">
            <field name="name">Inbound Transport</field>
            <field name="code">transport.order.in</field>
            <field name="prefix">%(y)s/%(hqcode)s/%(instance)s/ITO</field>
            <field name="padding">5</field>
            <field name="implementation">psql</field>
        </record>

        <record id="seq_type_transport_out" model="ir.sequence.type">
            <field name="name">Outbound Transport</field>
            <field name="code">transport.order.out</field>
        </record>

        <record id="seq_transport_out" model="ir.sequence">
            <field name="name">Outbound Transport</field>
            <field name="code">transport.order.out</field>
            <field name="prefix">%(y)s/%(hqcode)s/%(instance)s/OTO</field>
            <field name="padding">5</field>
            <field name="implementation">psql</field>
        </record>
    </data>
</openerp>
