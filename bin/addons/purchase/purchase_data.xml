            <field name="sequence">15</field>
        </record>
        <record id="pol_state_validated" model="purchase.order.line.state">
            <field name="name">validated</field>
            <field name="sequence">20</field>
        </record>
        <record id="pol_state_sourced" model="purchase.order.line.state">
            <field name="name">sourced</field>
            <field name="sequence">40</field>
        </record>
        <record id="pol_state_sourced_sy" model="purchase.order.line.state">
            <field name="name">sourced_sy</field>
