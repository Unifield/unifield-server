    <!-- ========================= UPDATES Sent & Received LINKS =============================== -->

        <!-- JI menu -->
        <record id="action_aml_open_update_sent" model="ir.actions.server">
            <field name="name">Updates Sent</field>
            <field name="model_id" ref="model_account_move_line"/>
            <field name="state">code</field>
            <field name="code">action = obj.open_update_sent(context=context)</field>
        </record>
        <record id="ir_aml_open_update_sent" model="ir.values">
            <field name="key2">client_action_relate</field>
            <field name="model">account.move.line</field>
            <field name="name">Updates Sent</field>
            <field eval="'ir.actions.server,%d'%action_aml_open_update_sent" name="value"/>
            <field eval="True" name="object"/>
            <field name="sequence" eval="100010"/> <!-- after the Track Changes -->
        </record>
